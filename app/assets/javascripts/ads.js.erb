$(document).ready(function(){

  $('[name=q]').on('keyup', function(e){
    e.preventDefault
    var query =$('[name=q]').val()

      if(query.length > 2){
          $.ajax({
              type: "GET",
              url: "/ads",
              data: { q: query},
              dataType: "script"
          })
      }

      else {
        $.ajax({
            type: "GET",
            url: "/ads",

            dataType: "script"

      }
  })

})
